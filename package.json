{
  "name": "file-system",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "jest": "^25.1.0",
    "mock-fs": "^4.10.3",
    "npm-watch": "^0.6.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.4"
  },
  "bin": {
    "extract": "echo extract"
  },
  "watch": {
    "run:processing": {
      "patterns": [
        "src"
      ],
      "extensions": "ts"
    }
  },
  "scripts": {
    "startprocessing": "npm run build:processing && node src/processing/processing.js",
    "build:processing": "tsc src/processing/*.ts",
    "run:processing": "npm-watch startprocessing",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles"
  },
  "dependencies": {
    "all-unpacker": "^0.1.3",
    "awesome-unoconv": "^1.0.1",
    "aws-sdk": "^2.589.0",
    "directory-tree": "^2.2.4",
    "dotenv": "^8.2.0",
    "edge-js": "^13.0.1",
    "file-type": "^12.4.0",
    "libreoffice-convert": "^1.0.4",
    "lodash": "^4.17.15",
    "mmmagic": "^0.5.3",
    "mongodb": "^3.5.2",
    "node-7z": "^2.0.4",
    "node-msoffice-pdf": "^0.0.7",
    "node-stream-zip": "^1.8.2",
    "node-unrar-js": "^0.8.1",
    "office-converter": "^1.0.2",
    "onezip": "^4.0.2",
    "read-chunk": "^3.2.0",
    "request": "^2.88.0",
    "tar": "^5.0.5",
    "unrar": "^0.2.0",
    "unzipper": "^0.10.5"
  }
}
